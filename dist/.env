# 服務器配置
watch_dog_url="http://192.168.1.8:8091"
pre_wd_login_url="http://localhost:8080"

gui_no="op1"

# 初始模式
init_state="Manual"
init_stroke_width=6
select_stroke_width=8
rect_without_mouse_rect_size=60
min_box_scale=[20, 20]

# 背景顏色
# 0 => 白色主題
# 1 => 黑色主題
bg_color=1

# 默認的自動、手動模式的倒計時 : s
Auto_time=6
Manual_time=60

# 是否开启清理缓存的功能
# 0 => 不開啟
# 1 => 開啟
clean_cache=1

# 缓存清理执行间隔: ms
clean_cache_interval=20000

save_ai_result=0
ai_result_path=""

# 后道btn的列表
post_result_list=["Confirm", "BlackRows"]

# x,y轴的方向
# reverse; eg: 6, 5, 4, 3, 2, 1 
# forward; eg: 1, 2, 3, 4, 5, 6
axisY=reverse
axisX=forward

# pre => 前道, 有vi
# post => 後道，无vi
line_position=post

# 是否开启远程Vi ，小写
# false => 不開啟
# true  => 開啟
isRemoteViShow=false

#远程vi的主机地址
viHost="http://192.168.2.28:6888"


# EL历史图片的保存位置 
ELPicSavedPath=C:\Users\yc-pc\Pictures\el_save

# 是否生成new gui 文件
# 0 no; 1 yes 
newGUIFiles=0

# vi历史图片保存位置
# 本项仅当 isRemoteViShow=false, line_position=pre 时才有意义
VIPicSavedPath=D:\yc\YC_GUIs\viHistory


# Space快捷键 触发范围
# global => 全局触发，此时在任何位置的空格操作都将触发 Space 快捷键操作
# local  => 只有当程序获得焦点时，触发相应程序的 Space 快捷键操作
spaceTriggerRange=local
